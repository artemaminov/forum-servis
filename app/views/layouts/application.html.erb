  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<header>
  <div class="navigation-svg">
    <div class="row">
      <div class="cell">
        <object data="assets/navigation-left.svg" type="image/svg+xml" id="navigation-left" class="fleft cell"></object>
      </div>
      <div class="cell mid">
      <object data="assets/navigation.svg" type="image/svg+xml" id="navigation" class="fleft cell"></object>
      </div>
      <div class="cell">
      <object data="assets/navigation-right.svg" type="image/svg+xml" id="navigation-right" class="fleft cell"></object>
      </div>
    </div>
  </div>
  <div class="header">
    <div class="logo-forum-big fleft"></div>
    <div class="info-block fleft lborder">
      <p class="status">Официальный дилер BRP в Тюмени</p>
      <p class="phone">(3452) 68-55-44</p>
    </div>
  </div>
  <div class="logo-brp-big fright"></div>
  <nav>
    <ul>
      <li><a href="#events">События</a></li>
      <li><a href="#brands">Бренды</a></li>
      <li><a href="#about">О компании</a></li>
      <li><a href="#contacts">Контакты</a></li>
    </ul>
  </nav>
</header>

<div class="carousel btcf">
  <ul>
    <li><img src="assets/ad1.jpg" alt=""/></li>
  </ul>
</div>

